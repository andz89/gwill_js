<%- include('../include/admin-header') %>


    <div class="contact-container admin-contact-container bg-dark ">

        <div class="contact-content d-flex justiy-content-start flex-direction-column lign-items-start">
            <div style="height: 10px; margin-top: -10px;">
                <div class="btn btn-sm btn-success update-contact-message" style=" display: none;">Update
                    Successfully!</div>
            </div>

            <div class="contact-body d-flex justify-content-start flex-direction-column gap-20 align-items-start "
                style="margin-top: 15px;">
                <span class="">Contact Us</span>

                <div class="d-flex   align-items-center gap-10">
                    <img src="images/phone.png" width="19" alt="" class="" />
                    <div>
                        <label for="">Phone Number</label>
                        <input type="text" id="phone_number" value="<%= phone_number %>">
                    </div>

                </div>
                <div class="d-flex  align-items-center just gap-10 my-1">
                    <img src="images/email.png" width="20" alt="" class="" />
                    <div>
                        <label for="">email</label>
                        <input type="email" id="email" value="<%= email %>">
                    </div>

                </div>
                <div class="d-flex  align-items-center just gap-10">
                    <img src="images/fb.png" width="20" alt="" class="" />
                    <div>
                        <label for="">Social Media</label>
                        <input type="text" id="facebook_page" value="<%= facebook %>">
                    </div>

                </div>

                <div class="d-flex  align-items-center just gap-10">
                    <img src="images/location.png" width="22" alt="" class="" />
                    <div>
                        <label for="">address</label>
                        <input type="text" id="address" value="<%= address %>">

                    </div>
                </div>

            </div>

        </div>
        <div class="d-flex justify-content-end ">

            <div id="update-contact-btn" class="btn btn-sm btn-primary btn-md">Update Contacct</div>

        </div>
        <br>
    </div>
    <%- include('../include/footer') %>